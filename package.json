{
  "name": "hyperlight-nanvix",
  "version": "0.1.0",
  "description": "Node.js bindings for hyperlight-nanvix - Run JavaScript, Python, C, and C++ in Nanvix sandboxes",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "hyperlight-nanvix",
    "features": ["napi"],
    "targets": [
      "x86_64-unknown-linux-gnu"
    ]
  },
  "license": "Apache-2.0",
  "files": [
    "*.node",
    "index.js",
    "index.d.ts"
  ],
  "devDependencies": {
    "@napi-rs/cli": "^3.4.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "napi build --platform --release --features napi",
    "build:debug": "napi build --platform --features napi"
  },
  "keywords": [
    "hyperlight",
    "nanvix",
    "sandbox", 
    "javascript",
    "python",
    "binary",
    "vm",
    "microkernel"
  ]
}